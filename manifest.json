{
	"description": "Select multiple links, checkboxes and other elements and do something with them such as open them in new tabs or check/uncheck them.",
	"manifest_version": 2,
	"name": "Snap Links",
	"version": "3.0.0.1",
	"version_name": "3.0b1",
	"homepage_url": "https://github.com/cpriest/SnapLinksPlus",
	"icons": {
	},
	"applications": {
		"gecko": {
			"id": "snaplinks@dev.rxv.me",
			"strict_min_version": "45.0.0"
		}
	},
	"background": {
		"scripts": [
			"src/globals.es6",
			"src/background-scripts/background.es6"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"src/globals.es6",
				"src/content-scripts/Utility.es6",
				"src/content-scripts/SnapLinks.es6",
				"src/content-scripts/ElementIndexer.es6",
				"src/content-scripts/ElementHighlighter.es6"
			]
		}
	]
}
